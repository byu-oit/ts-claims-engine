{
  "name": "ts-claims-engine",
  "version": "0.0.0",
  "description": "Claims engine implementation in TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/tslint -p tsconfig.json -t stylish --fix",
    "build": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/mocha -r ts-node/register ./test/**/*-test.ts",
    "coverage": "./node_modules/.bin/nyc ./node_modules/.bin/mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byu-oit/ts-claims-engine.git"
  },
  "author": "Spencer Tuft <spencer_tuft@byu.edu>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/byu-oit/ts-claims-engine/issues"
  },
  "homepage": "https://github.com/byu-oit/ts-claims-engine#readme",
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.3",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "@byu-oit/byuapi": "0.0.13",
    "@types/express": "^4.17.1",
    "change-case": "^3.1.0",
    "openapi-enforcer": "^1.5.1",
    "openapi-enforcer-middleware": "^1.1.6"
  }
}
